{
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "svg.preview.background": "editor",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/node_modules": true,
        "**/logs": true,
    },
    "files.associations": {
        ".env.template": "ini",
        ".env": "ini"
    }
}